<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view2"
        android:layout_width="415dp"
        android:layout_height="43dp"
        android:background="#0A3BCD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="138dp"
        android:layout_height="31dp"
        android:foregroundTint="#FFFFFF"
        android:text="SETTINGS"
        android:textColor="#FFFFFF"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="11dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="85dp"
        android:layout_height="42dp"
        android:text="Exit"
        tools:layout_editor_absoluteX="329dp"
        tools:layout_editor_absoluteY="1dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="85dp"
        android:layout_height="42dp"
        android:text="Back"
        tools:layout_editor_absoluteX="244dp"
        tools:layout_editor_absoluteY="0dp" />

    <EditText
        android:id="@+id/userName"
        android:layout_width="350dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="113dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Name:"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="94dp" />

    <EditText
        android:id="@+id/email"
        android:layout_width="350dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="207dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="188dp" />

    <EditText
        android:id="@+id/repeatedPassword"
        android:layout_width="350dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="389dp" />

    <EditText
        android:id="@+id/password"
        android:layout_width="350dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="295dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password:"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="276dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confirm password:"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="370dp" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="413dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Personal settings"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="42dp" />

    <EditText
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="413dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="App settings"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="444dp" />

    <Switch
        android:id="@+id/captureVideos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Capture videos"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="555dp" />

    <Switch
        android:id="@+id/captureFotos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Capture photos"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="517dp" />

    <Switch
        android:id="@+id/lifeStream"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Life Stream Capture"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="555dp" />

    <Switch
        android:id="@+id/sendNotifications"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send notifications"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="517dp" />

    <Button
        android:id="@+id/saveUpdate"
        android:layout_width="275dp"
        android:layout_height="64dp"
        android:layout_marginBottom="44dp"
        android:text="Save &amp; update"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>